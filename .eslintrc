{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "babel",
    "react-hooks"
  ],
  "env": {
    "browser": true,
  },
  "rules": {
    "arrow-parens": "off",
    "import/no-unresolved": "off",
    "max-len": ["error", 250],
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off"
  }
}
